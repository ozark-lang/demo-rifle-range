inheritance Object

property @casing:BulletCasing?
property @used:Boolean

extension initialize
	create casing:BulletCasing; initialize
	
	set @casing <- casing
	
method getCasing -> _$casing:BulletCasing?
	with casing:@casing
		set $casing <- casing

method hitTarget _target:Target
	clear @casing

	target getHit