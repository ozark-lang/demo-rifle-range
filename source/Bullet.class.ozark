inheritance Object

requirement Target

property @casing:BulletCasing?
property @used:Boolean

extension initialize
	create casing:BulletCasing; initialize
	
	assign @casing <- casing

method getCasing -> $casing:BulletCasing?
	with @casing
		assign casing <- @casing

method hitTarget $target:Target
	clear @casing

	target getHit