class Bullet
	inheritance Object reference:"Object"

	requirement Target source:"Target"

	member casing:BulletCasing?

	property used:Boolean

	continuation initialize
		create casing1:BulletCasing initialize
		set .casing <- casing1

	method casing -> output:BulletCasing
		set output <- casing!
			catch e:NoValueException
				e setMessage "The bullet has already been shot."
				throw e

	method hitTarget input:Target
		unset .casing
		input getHit