inheritance Object

property @clip:[Bullet]

extension initialize
	for i:[1~4] -> clip:[Bullet]
		create bullet:Bullet; initialize

		set clip[$] <- bullet

	set @clip <- clip

method shootTarget _target:Target -> $casing:Bullet.Casing? $smoke:Smoke?
	if @clip[#]
		@clip[-1] getCasing -> casing
		@clip[-1] hitTarget target

		create smoke:Smoke; initialize

		set $smoke <- smoke
		set $casing <- casing

		set @clip <- @clip[1~-2]